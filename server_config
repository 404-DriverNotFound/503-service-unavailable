server
	root ./root
	server_name localhost
	port 9190
	error_page error.html
	index index.html
	head_length 8000
	body_length 1000000
	autoindex on
	timeout 10
	location /
		method GET
	location /put_test
		root /
		index index.html index.php index.txt
		method PUT
		cgi script.php
	location /post_body
		root /
		index index.html index.php index.txt
		method POST PUT GET
		cgi script.php
	location /directory
		root /YoupiBanane
		index youpi.bad_extension
		method GET
		body_length 100
		cgi script.php