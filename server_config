server
	root ./root
	server_name localhost
	port 9191
	error_page error.html
	index index.html
	head_length 8000
	body_length 10000000
	autoindex on
	timeout 10
	location /
		method GET
		index index.html index.php index.txt
	location /put_test
		root /
		index index.html index.php index.txt
		method PUT
		cgi_extension .php .bla
	location /post_body
		root /
		index index.html index.php index.txt
		method POST PUT GET
		cgi_extension .php .bla
		body_length 100
	location /directory
		root /YoupiBanane
		index youpi.bad_extension
		method GET POST
		cgi_extension .php .bla